@model IEnumerable<BorrowEquipmentApp.Models.EquipmentModel>  // Change to your actual model namespace

@{
    ViewData["Title"] = "All Equipment Listing";
}

<h1>All Equipment Listing</h1>

<table class="table">
    <thead>
        <tr>
            <th>Equipment ID</th>
            <th>Type</th>
            <th>Description</th>
            <th>Availability</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var equipment in Model)
        {
            <tr>
                <td>@equipment.Id</td>
                <td>@equipment.Type</td>
                <td>@equipment.Description</td>
                <td>@(equipment.Availability ? "Available" : "Not Available")</td>
            </tr>
        }
    </tbody>
</table>
